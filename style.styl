@import 'styl/*'

add(a, b, c = a)
  a + b + c

marg(a, b)
  return a a*10 a*b


body
  background blue
  color red
  border solid 1px @color
  margin-left add(1px,2)
  main
    margin marg(10px, 20)
    color blue
    transition(all 0.2s ease)
    border solid 1px @color
    border-radius 20px
    test(11)
    cf()